class Solution {
public:
    string makeFancyString(string s) {
        int len=s.length();
        int i=0;
        string s2;
        for(int i=0;i<len;i++){
            if(s[i]==s[i+1] && s[i]==s[i+2]){
                continue;
            }
            else
            s2+=s[i];
        }
        return s2;
    }
};
